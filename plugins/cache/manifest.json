{
  "background": {
    "service_worker": "background.js"
  },
  "description": "Deeeep.io Cache",
  "manifest_version": 3,
  "name": "Deeeep.io Cache",
  "permissions": ["webRequest", "storage", "declarativeNetRequest"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rule.json"
      }
    ]
  },
  "host_permissions": ["https://deeeep.io/*", "https://beta.deeeep.io/*"],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1"
}
